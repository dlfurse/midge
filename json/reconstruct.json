    "line_transformer" :
    {
        "name" : "line",
        "threshold" : 1.5,
        "window" : 65.0e3,
        "width" : 65.0e3,
        "count" : 50,
        "hit_factor" : 1.0,
        "hit_power" : 2.0,
        "miss_factor" : 10.0,
        "miss_power" : 3.0,
        "score_low" : 0.0,
        "quality_high" : 500.0,
        "length" : 10
    },

    "line_plot_consumer" :
    {
        "name" : "line_plot",
        "plot_overlay" : false,
        "plot_field" : "quality",
