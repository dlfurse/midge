    "rtf_binary_producer" :
    {
        "name" : "signal",
        "file" : "signal_spectrum.mgb",
        "length" : 10
    },

    "point_transformer" :
    {
        "name" : "unused",
        "begin_sec" : 0.0,
        "end_sec" : 1.0,
        "begin_hz" : 5.0e6,
        "end_hz" : 120.0e6,
        "length" : 10
    },

    "point_transformer" :
    {
        "name" : "point",
        "begin_sec" : 0.065,
        "end_sec" : 0.088,
        "begin_hz" : 52.0e6,
        "end_hz" : 80.0e6,
        "length" : 10
    },

    "point_plot_consumer" :
    {
        "name" : "point_plot",
        "plot_field" : "ratio",
        "plot_key" : "point_canvas",
        "plot_name" : "point_histogram",
        "chart_title" : "Points",
        "x_title" : "Time [sec]",
        "y_title" : "Frequency [hertz]",
        "z_title" : "Ratio [none]"
