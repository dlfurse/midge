{
    "rf_binary_producer" :
    {
        "name" : "background",
        "file" : "<background>",
        "length" : 10
    },

    "rtf_binary_producer" :
    {
        "name" : "signal",
        "file" : "<signal>",
        "length" : 10
    },

    "point_transformer" :
    {
        "name" : "point",
        "threshold" : 1.5,
        "high_factor" : 1.0,
        "high_power" : 1.5,
        "low_factor" : 3.0,
        "low_power" : 1.0,
        "begin_sec" : 0.0,
        "end_sec" : 10.0,
        "begin_hz" : 15.0e6,
        "end_hz" : 110.0e6,
        "length" : 20
    },

    "point_plot_consumer" :
    {
        "name" : "point_plot",
        "plot_field" : "score",
        "plot_key" : "point_canvas",
        "plot_name" : "point_histogram",
        "chart_title" : "Points",
        "x_title" : "Time [sec]",
        "y_title" : "Frequency [hertz]",
        "z_title" : "Score [none]"
    },

    "line_transformer" :
    {
        "name" : "line",
        "threshold" : 5.0,
        "window" : 65.0e3,
        "width" : 35.0e3,
        "count" : 30,
        "score_low" : 0.0,
        "quality_high" : 100.0,
        "length" : 10
    },

    "line_plot_consumer" :
    {
        "name" : "line_plot",
        "plot_overlay" : false,
        "plot_field" : "quality",
        "plot_key" : "line_canvas",
        "plot_name" : "line_histogram",
        "chart_title" : "Lines",
        "x_title" : "Time [sec]",
        "y_title" : "Frequency [hertz]",
        "z_title" : "ID [none]"
    },

    "join" : "background.out_0:point.in_0",
    "join" : "signal.out_0:point.in_1",
    "join" : "point.out_0:line.in_0",
    "join" : "line.out_0:line_plot.in_0",

    "run" : "background:signal:point:line:line_plot"
}
