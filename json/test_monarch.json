{
    "rt_data" :
    {
        "name" : "rtw"
    },

    "rt_harmonic_producer" :
    {
        "name" : "rtw_in",
        "power_dbm" : -4.0,
        "impedance_ohm" : 50.0,
        "frequency_hz" : 1.0e6,
        "phase_deg" : 270.0,
        "start_sec" : 0.0e-9,
        "stop_sec" : 20000.0e-9,
        "interval" : 1.0e-9,
        "size" : 10000,
        "stride" : 100
    },

    "rt_root_consumer" :
    {
    	"name" : "rtwa_out",
    	"file" : "test_monarch.write.root",
        "plot" : true,
        "plot_key" : "monarch_write_canvas",
        "plot_name" : "monarch_write_histogram",
        "chart_title" : "Test Monarch Write",
        "axis_title" : "Signal [volt]"
    },

    "rt_monarch_consumer" :
    {
    	"name" : "rtwm_out",
    	"file" : "test_monarch.egg",
    	"bits" : 8,
    	"length" : 5000,
    	"voltage_minimum" : -0.25,
    	"voltage_maximum" : 0.25,
    	"duration" : 20000.0e-9
    },

    "void_controller" :
    {
    	"name" : "cw"
    },

    "join" : "rtw_in.out_0:rtw.in",
    "join" : "rtw.out:rtwa_out.in_0",
    "join" : "rtw.out:rtwm_out.in_0",
    "join" : "cw.out_0:rtw_in",

    "run" : "cw",

    "rt_data" :
    {
        "name" : "rtr"
    },

    "rt_monarch_producer" :
    {
        "name" : "rtr_in",
        "file" : "test_monarch.egg",
        "minimum_time" : 0.0e-9,
        "maximum_time" : 20000.0e-9,
        "size" : 100,
        "stride" : 200
    },

    "rt_root_consumer" :
    {
        "name" : "rtra_out",
        "file" : "test_monarch.read.root",
        "plot" : true,
        "plot_key" : "monarch_read_canvas",
        "plot_name" : "monarch_read_histogram",
        "chart_title" : "Test Monarch Read",
        "axis_title" : "Signal [volt]"
    },

    "void_controller" :
    {
        "name" : "cr"
    },

    "join" : "rtr_in.out_0:rtr.in",
    "join" : "rtr.out:rtra_out.in_0",
    "join" : "cr.out_0:rtr_in",

    "run" : "cr"
}
