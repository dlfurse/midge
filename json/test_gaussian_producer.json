{
    "rt_data" :
    {
        "name" : "rt"
    },

    "ct_data" :
    {
        "name" : "ct"
    },

    "cf_data" :
    {
        "name" : "cf"
    },

    "rf_data" :
    {
        "name" : "rf"
    },

    "rt_gaussian_producer" :
    {
        "name" : "rt_in",
        "power_dbm" : -10.0,
        "impedance_ohm" : 50.0,
        "start_sec" : 100.0e-9,
        "stop_sec" : 1900.0e-9,
        "interval" : 1.0e-9,
        "seed" : 51385,
        "size" : 2000,
        "stride" : 2000
    },

    "rt_ct_transformer" :
    {
        "name" : "rt_ct",
        "mode" : "real"
    },

    "ct_cf_fourier_transformer" :
    {
        "name" : "ct_cf"
    },

    "cf_rf_power_transformer" :
    {
        "name" : "cf_rf",
        "impedance_ohm" : 50.0
    },

    "rt_ascii_consumer" :
    {
        "name" : "rt_out",
        "file" : "test_gaussian_producer.signal.txt"
    },

    "rf_ascii_consumer" :
    {
        "name" : "rf_out",
        "file" : "test_gaussian_producer.spectrum.txt"
    },

    "join" : "rt_in.out_0:rt.in",
    "join" : "rt.out:rt_ct.in_0",
    "join" : "rt_ct.out_0:ct.in",
    "join" : "ct.out:ct_cf.in_0",
    "join" : "ct_cf.out_0:cf.in",
    "join" : "cf.out:cf_rf.in_0",
    "join" : "cf_rf.out_0:rf.in",
    "join" : "rt.out:rt_out.in_0",
    "join" : "rf.out:rf_out.in_0",

    "run" : "rt_in"
