{
    "rt_monarch_producer" :
    {
        "name" : "signal",
        "file" : "simulation_event_signal.egg",
        "begin_sec" : 0.0,
        "end_sec" : 2.0e-4,
        "size" : 1000,
        "stride" : 1000,
        "length" : 10
    },

    "rt_rtf_power_transformer" :
    {
        "name" : "power",
        "impedance_ohm" : 50,
        "window_rectangular" :
        {
        },
        "length" : 10
    },

    "rtf_rtf_ratio_transformer" :
    {
        "name" : "event",
        "file" : "simulation_background_spectrum.mgb",
        "length" : 10
    },

    "rtf_binary_consumer" :
    {
        "name" : "binary",
        "file" : "simulation_event_spectrum.mgb"
    },

    "rtf_plot_consumer" :
    {
        "name" : "plot",
        "plot_key" : "spectrum_out_canvas",
        "plot_name" : "spectrum_out_histogram",
        "chart_title" : "Event Spectrum",
        "x_title" : "Time [sec]",
        "y_title" : "Frequency [hertz]",
        "z_title" : "Wigner Amplitude [none]"
    },

    "join" : "signal.out_0:power.in_0",
    "join" : "power.out_0:event.in_0",
    "join" : "event.out_0:binary.in_0",
    "join" : "event.out_0:plot.in_0",

    "run" : "signal:power:event:binary:plot"
}
