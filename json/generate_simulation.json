{
    "rt_data" :
    {
        "name" : "rtc"
    },

    "rt_data" :
    {
        "name" : "rtg"
    },

    "rt_data" :
    {
        "name" : "rts"
    },

     "rt_chirp_producer" :
    {
        "name" : "rtc_in",
        "power_dbm" : -30.0,
        "impedance_ohm" : 50.0,
        "start_frequency_hz" : 100.0e6,
        "stop_frequency_hz" : 400.0e6,
        "phase_deg" : 60.0,
        "start_sec" : 0.2e-4,
        "stop_sec" : 1.8e-4,
        "interval" : 1.0e-9,
        "size" : 1000,
        "stride" : 1000
    },

    "rt_gaussian_producer" :
    {
        "name" : "rtg_in",
        "power_dbm" : -12.0,
        "impedance_ohm" : 50.0,
        "start_sec" : 0.0e-4,
        "stop_sec" : 2.0e-4,
        "interval" : 1.0e-9,
        "size" : 1000,
        "stride" : 1000
    },

    "rt_rt_sum_transformer" :
    {
        "name" : "rts_in"
    },

    "rt_monarch_consumer" :
    {
        "name" : "rtm_out",
        "file" : "trap_simulation.egg",
        "bits" : 8,
        "length" : 5000,
        "voltage_minimum" : -0.25,
        "voltage_maximum" : 0.25,
        "duration" : 2.0e-4
    },

    "rt_root_consumer" :
    {
        "name" : "rtr_out",
        "file" : "trap_simulation.root",
        "plot" : true,
        "plot_key" : "rtr_out_canvas",
        "plot_name" : "rtr_out_histogram",
        "chart_title" : "Simulated Signal",
        "axis_title" : "Signal [volt]"
    },

    "void_controller" :
    {
        "name" : "c"
    },

    "join" : "rtc_in.out_0:rtc.in",
    "join" : "rtc.out:rts_in.in_0",
    "join" : "rtg_in.out_0:rtg.in",
    "join" : "rtg.out:rts_in.in_1",
    "join" : "rts_in.out_0:rts.in",
    "join" : "rts.out:rtr_out.in_0",
    "join" : "rts.out:rtm_out.in_0",
    "join" : "c.out_0:rtc_in",
    "join" : "c.out_1:rtg_in",

    "run" : "c"
}