{
    "rt_data" : 
    {
        "name" : "signal_data"
    },

    "rf_data" :
    {
        "name" : "power_data"
    },

    "rt_monarch_producer" :
    {
        "name" : "signal",
        "file" : "data_background_signal.egg",
        "minimum_time" : 0.0,
        "maximum_time" : 1.0,
        "size" : 8192,
        "stride" : 8192
    },

    "rt_rf_power_transformer" :
    {
        "name" : "power",
        "impedance_ohm" : 50,
        "window_rectangular" :
        {
        }
    },

    "rf_background_consumer" :
    {
        "name" : "background",
        "file" : "data_background.root",
        "length" : 60,
        "window_hanning" :
        {
        },
        "plot" : false,
        "plot_key" : "background_canvas",
        "plot_name" : "background_histogram",
        "chart_title" : "Data Spectrum",
        "axis_title" : "Power [watt]"
    },

    "void_controller" :
    {
        "name" : "control"
    },

    "join" : "signal.out_0:signal_data.in",
    "join" : "signal_data.out:power.in_0",
    "join" : "power.out_0:power_data.in",
    "join" : "power_data.out:background.in_0",
    "join" : "control.out_0:signal",

    "run" : "control"
}
