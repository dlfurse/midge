{
    "rt_data" :
    {
        "name" : "signal_data"
    },

    "rf_data" :
    {
        "name" : "power_data"
    },

    "rf_data" :
    {
        "name" : "threshold_data"
    },

    "rt_monarch_producer" :
    {
        "name" : "signal",
        "file" : "trap_on.egg",
        "minimum_time" : 0.308,
        "maximum_time" : 0.317,
        "size" : 8192,
        "stride" : 4096
    },

    "rt_rf_power_transformer" :
    {
        "name" : "power",
        "impedance_ohm" : 50,
