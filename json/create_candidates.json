    {
        "name" : "threshold",
        "threshold_file" : "trap_threshold.root",
        "background_file" : "trap_background.root",
        "threshold" : 4.0,
        "minimum_frequency" : 41.0e6,
        "maximum_frequency" : 48.0e6,
        "plot" : true,
        "plot_key" : "analysis_canvas",
        "plot_name" : "threshold_histogram",
        "chart_title" : "Thresholds",
        "axis_title" : "Excess Power Ratio"
